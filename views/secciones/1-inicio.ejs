<section id="imagenes">
	<ul>
		<% for (n of inicio_imagenes) { %>
			<li>
				<% if (typeof n == "string") { %>
					<img id="una" src="./imagenes/1-inicio/<%= n %>.jpg">
				<% } else { %>
					<div id="varias">
						<% for (m of n) { %>
							<img src="./imagenes/1-inicio/<%= m %>.jpg">
						<% } %>
					</div>
				<% } %>
			</li>
		<% } %> 
		<li>
			<img id="una" src="./imagenes/1-inicio/<%= inicio_imagenes[0] %>.jpg">
		</li>
	</ul>
	<div id="iconos">
		<i class="fas fa-chevron-left ocultar"></i>
		<i class="fas fa-pause-circle"></i>
		<i class="fas fa-play-circle ocultar"></i>
