<h2><%= n.titulo_seccion %></h2>
<section id="arquitecto">
	<h3>
		<%= quienes_somos.find(m => (m.grupo == 1 && m.orden == 1)).contenido %>
		<%- include("avatar", {ancho: "ancho"}) %>
	</h3>
	<%- include("avatar", {ancho: "angosto"}) %>
	<ul>
		<% textos = quienes_somos.filter(m => (m.grupo == 1 && m.orden > 1)) %>
		<% for (texto of textos) { %>
			<li><%= texto.contenido %></li>
		<% } %>
	</ul>
	<a href="https://www.cpau.org/Content/institucional/ficha/6ecf7056-ce07-4b9c-8267-b2c899ff1799" target="_blank">
		<button style="background-color:<%= n.color_fondo_boton.codigo %>; color:<%= n.color_letras_boton.codigo %>; border-color:<%= n.color_borde_boton.codigo %>">
			<%= quienes_somos.find(m => m.grupo == 2 && m.orden == 1).contenido %>
		</button>
	</a>
</section>
<section id="clientes">
	<h3><%= quienes_somos.find(m => m.grupo == 3 && m.orden == 1).contenido %></h3>
	<ul>
		<% for (j of clientes_imagenes) { %>
			<li>
				<img src="/externa/5-quienes_somos/<%= j.archivo %>" alt="<%= j.nombre %>">
			</li>
		<% } %>
	</ul>
</section>